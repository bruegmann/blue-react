{
  "name": "blue-react",
  "version": "6.9.2",
  "description": "Blue App als React Components",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "webpack -d --watch",
    "test": "webpack -d --watch",
    "test-build": "webpack -p",
    "build": "babel ./src --out-dir ./dist & node-sass ./dist/style.scss ./dist/style.css",
    "doc": "react-docgen ./src -o ./doc/src/doc.json && node ./followUpDoc.js",
    "doc-doc": "react-docgen ./doc/src -o ./doc/src/doc-doc.json",
    "start": "npm run watch",
    "put-version": "put-version ./doc/index.html",
    "build-all": "npm run test-build && npm run build && npm run doc && npm run doc-doc && npm run put-version",
    "deploy-only": "npm publish --tag legacy",
    "deploy": "npm run build-all && npm run deploy-only"
  },
