{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false // Verhindert die Transformation von ES-Modules
            }
        ],
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ],
        [
            "@babel/preset-typescript",
            {
                "isTSX": true,
                "allExtensions": true
            }
        ]
    ],
    "plugins": ["babel-plugin-add-import-extension"]
}
